  keywords = {climate,Climate Change,consumption,environment,extinction,Human population,Political will,sustainability},
  file = {/home/eamonn/Zotero/storage/BGZBTADG/Bradshaw et al. - 2021 - Underestimating the Challenges of Avoiding a Ghast.pdf}

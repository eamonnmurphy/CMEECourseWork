  file = {/home/eamonn/Zotero/storage/G827M934/Ling et al. - 2013 - CCAT2, a novel noncoding RNA mapping to 8q24, unde.pdf;/home/eamonn/Zotero/storage/GHGY2CQL/1446.html}
}

@article{linnerEpigenomewideAssociationStudy2017,
  title = {An Epigenome-Wide Association Study Meta-Analysis of Educational Attainment},
  author = {Linn{\'e}r, R. Karlsson and Marioni, R. E. and Rietveld, C. A. and Simpkin, A. J. and Davies, N. M. and Watanabe, K. and Armstrong, N. J. and Auro, K. and Baumbach, C. and Bonder, M. J. and Buchwald, J. and Fiorito, G. and Ismail, K. and Iurato, S. and Joensuu, A. and Karell, P. and Kasela, S. and Lahti, J. and McRae, A. F. and Mandaviya, P. R. and Sepp{\"a}l{\"a}, I. and Wang, Y. and Baglietto, L. and Binder, E. B. and Harris, S. E. and Hodge, A. M. and Horvath, S. and Hurme, M. and Johannesson, M. and Latvala, A. and Mather, K. A. and Medland, S. E. and Metspalu, A. and Milani, L. and Milne, R. L. and Pattie, A. and Pedersen, N. L. and Peters, A. and Polidoro, S. and R{\"a}ikk{\"o}nen, K. and Severi, G. and Starr, J. M. and Stolk, L. and Waldenberger, M. and Eriksson, J. G. and Esko, T. and Franke, L. and Gieger, C. and Giles, G. G. and H{\"a}gg, S. and Jousilahti, P. and Kaprio, J. and K{\"a}h{\"o}nen, M. and Lehtim{\"a}ki, T. and Martin, N. G. and van Meurs, J. B. C. and Ollikainen, M. and Perola, M. and Posthuma, D. and Raitakari, O. T. and Sachdev, P. S. and Taskesen, E. and Uitterlinden, A. G. and Vineis, P. and Wijmenga, C. and Wright, M. J. and Relton, C. and Smith, G. Davey and Deary, I. J. and Koellinger, P. D. and Benjamin, D. J.},
  year = {2017},
  month = dec,
  journal = {Molecular Psychiatry},
  volume = {22},
  number = {12},
  pages = {1680--1690},
  issn = {1476-5578},
  doi = {10.1038/mp.2017.210},
  abstract = {The epigenome is associated with biological factors, such as disease status, and environmental factors, such as smoking, alcohol consumption and body mass index. Although there is a widespread perception that environmental influences on the epigenome are pervasive and profound, there has been little evidence to date in humans with respect to environmental factors that are biologically distal. Here we provide evidence on the associations between epigenetic modifications\textemdash in our case, CpG methylation\textemdash and educational attainment (EA), a biologically distal environmental factor that is arguably among the most important life-shaping experiences for individuals. Specifically, we report the results of an epigenome-wide association study meta-analysis of EA based on data from 27 cohort studies with a total of 10\,767 individuals. We find nine CpG probes significantly associated with EA. However, robustness analyses show that all nine probes have previously been found to be associated with smoking. Only two associations remain when we perform a sensitivity analysis in the subset of never-smokers, and these two probes are known to be strongly associated with maternal smoking during pregnancy, and thus their association with EA could be due to correlation between EA and maternal smoking. Moreover, the effect sizes of the associations with EA are far smaller than the known associations with the biologically proximal environmental factors alcohol consumption, body mass index, smoking and maternal smoking during pregnancy. Follow-up analyses that combine the effects of many probes also point to small methylation associations with EA that are highly correlated with the combined effects of smoking. If our findings regarding EA can be generalized to other biologically distal environmental factors, then they cast doubt on the hypothesis that such factors have large effects on the epigenome.},
  copyright = {2017 The Author(s)},
  langid = {english},
  file = {/home/eamonn/Zotero/storage/QYY4MY8Z/Linnér et al. - 2017 - An epigenome-wide association study meta-analysis .pdf;/home/eamonn/Zotero/storage/CSGT32IG/mp2017210.html}
}

@article{lira-noriegaRelationshipGeneticDiversity2014,
  title = {Relationship of {{Genetic Diversity}} and {{Niche Centrality}}: A {{Survey}} and {{Analysis}}},
  shorttitle = {Relationship of {{Genetic Diversity}} and {{Niche Centrality}}},
  author = {{Lira-Noriega}, Andr{\'e}s and Manthey, Joseph D.},
  year = {2014},
  journal = {Evolution},
  volume = {68},
  number = {4},
  pages = {1082--1093},
  issn = {1558-5646},
  doi = {10.1111/evo.12343},
  abstract = {The distribution of genetic diversity within and among populations in relation to species' geographic ranges is important to understanding processes of evolution, speciation, and biogeography. One hypothesis predicts that natural populations at geographic range margins will have lower genetic diversity relative to those located centrally in species' distributions owing to a link between geographic and environmental marginality; alternatively, genetic variation may be unrelated with geographic marginality via decoupling of geographic and environmental marginality. We investigate the predictivity of geographic patterns of genetic variation based on geographic and environmental marginality using published genetic diversity data for 40 species (insects, plants, birds, mammals, worms). Only about half of species showed positive relationships between geographic and environmental marginality. Three analyses (sign test, multiple linear regression, and meta-analysis of correlation effect sizes) showed a negative relationship between genetic diversity and distance to environmental niche centroid, but no consistent relationship of genetic diversity with distance to geographic range center.},
  langid = {english},
  keywords = {Central-peripheral hypothesis,ecological niche,genetic diversity,geographic range},
  annotation = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/evo.12343},
  file = {/home/eamonn/Zotero/storage/QVP8UZYP/Lira-Noriega and Manthey - 2014 - Relationship of Genetic Diversity and Niche Centra.pdf;/home/eamonn/Zotero/storage/B3MACRXB/evo.html}
}


@article{nagerActinNetworkDispatches2017,
  title = {An Actin Network Dispatches Ciliary {{GPCRs}} into Extracellular Vesicles to Modulate Signaling},
  author = {Nager, Andrew R. and Goldstein, Jaclyn S. and {Herranz-P{\'e}rez}, Vicente and Portran, Didier and Ye, Fan and {Garcia-Verdugo}, Jose Manuel and Nachury, Maxence V.},
